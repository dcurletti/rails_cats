<h1>Cats</h1>
<div id="cat_table">
  <table>
    <tr>
      <th>Name<th>
      <th>Color<th>
      <th>Sex<th>
      <th>Description<th>
      <th>Age<th>
    </tr>
    <% @cats.each do |cat| %>
      <tr>
        <td><%= cat.name %><td>
        <td><%= cat.color %><td>
        <td><%= cat.sex %><td>
        <td><%= cat.description %><td>
        <td><%= cat.age %><td>
      </tr>
    <% end %>
  </table>
</div>
