<!DOCTYPE html>
<html>
<head>
  <title>Cats99</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <header>
    <%= render  "layouts/header"%>
  </header>

  <% if flash.now[:errors] %>
  <% flash.now[:errors].each do |error| %>
  <%= error %><br />
  <% end %>
  <% end %>


  <main>
    <%= yield %>
  </main>

</body>
</html>
